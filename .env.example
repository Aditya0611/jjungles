# LinkedIn Hashtag Scraper - Environment Variables
# Copy this file to .env and fill in your values

# Supabase Configuration (Required for database storage)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
USE_SUPABASE=true

# Browser Configuration (Optional)
HEADLESS=false
BROWSER_LOCALE=en-US
BROWSER_TIMEZONE=America/New_York
BROWSER_LATITUDE=40.7128
BROWSER_LONGITUDE=-74.0060

# Proxy Configuration (Optional)
USE_PROXIES=false
PROXY_FILE=proxies.txt
ROTATE_PROXY_EVERY=10

# Scraper Configuration (Optional)
SCROLL_PAUSE_TIME=1.5
MAX_SCROLLS=50
OUTPUT_FILE=trending_hashtags.json

# LinkedIn Credentials (Optional - Manual login recommended)
# LINKEDIN_EMAIL=your-email@example.com
# LINKEDIN_PASSWORD=your-password
