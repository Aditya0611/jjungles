# Instagram Credentials (REQUIRED)
INSTAGRAM_USERNAME=your_username_here
INSTAGRAM_PASSWORD=your_password_here

# Supabase Configuration (REQUIRED)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_service_role_key_here

# Proxy Configuration (Recommended)
# Format: http://user:pass@host:port or http://host:port
PROXY_SERVER=
PROXY_USERNAME=
PROXY_PASSWORD=
# Supported strategies: health_based, round_robin, random
PROXY_ROTATION_STRATEGY=health_based
# Comma-separated list of proxy servers for rotation
PROXY_SERVERS=
# Path to file containing list of proxies (one per line)
PROXY_LIST_PATH=proxies.txt
# Strictly require proxies (fail if none configured)
REQUIRE_PROXIES=true

# Scraper Behavior Settings
# Run in headless mode (true/false)
HEADLESS=true
# How many times to scroll the explore page
SCROLL_COUNT=5
# Maximum posts to scan
POSTS_TO_SCAN=200
# Minimum frequency to consider a hashtag trending
MIN_HASHTAG_FREQUENCY=3
# Number of top trends to save to DB
TOP_HASHTAGS_TO_SAVE=10
# Number of posts to analyze per hashtag for engagement
POSTS_PER_HASHTAG=5

# Scheduling (for Odoo/Cron)
# Interval in hours
SCRAPE_INTERVAL_HOURS=6

# Logging
LOG_LEVEL=INFO
# Enable JSON structured logging
USE_JSON_LOGGING=true
