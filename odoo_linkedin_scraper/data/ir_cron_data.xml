<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_linkedin_scraper_action" model="ir.cron">
            <field name="name">LinkedIn Hashtag Scraper: Scheduled Run</field>
            <field name="model_id" ref="model_linkedin_scraper"/>
            <field name="state">code</field>
            <field name="code">model.cron_run_scraper()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
