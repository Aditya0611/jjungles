<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_run_twitter_scraper" model="ir.cron">
            <field name="name">Run Twitter Scraper (External)</field>
            <field name="model_id" ref="model_twitter_integrator"/>
            <field name="state">code</field>
            <field name="code">model.run_scraper()</field>
            <field name="interval_number">4</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
